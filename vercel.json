{
  "builds": [
    { "src": "verada/wsgi.py", "use": "@vercel/python" },
    {
            "src": "media/**",
            "use": "@vercel/static"
        }
  ],
  "routes": [
    { "src": "/static/(.*)", "dest": "staticfiles/$1" },
    { "src": "/media/(.*)", "dest": "media/$1" },
    { "src": "/(.*)", "dest": "verada/wsgi.py" }
  ],
   "static": ["media"]
}
